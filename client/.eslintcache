\nconst SocialLoginBlock = styled.div`\r\n  .fb-icon {\r\n    color: #3a559f;\r\n  }\r\n  .kk-icon {\r\n    color: #391b1b;\r\n    background-color: yellow;\r\n  }\r\n  svg {\r\n    font-size: 2rem;\r\n    margin: 5px;\r\n  }\r\n`;\r\n\r\nconst Login = (props) => {\r\n  const dispatch = useDispatch();\r\n  const [Id, setId] = useState(\"\");\r\n  const [Password, setPassword] = useState(\"\");\r\n\r\n  const onIdChange = (e) => {\r\n    setId(e.target.value);\r\n  };\r\n\r\n  const onPasswordChange = (e) => {\r\n    setPassword(e.target.value);\r\n  };\r\n\r\n  const onSubmit = async (e) => {\r\n    e.preventDefault();\r\n\r\n    let body = {\r\n      id: Id,\r\n      password: Password,\r\n    };\r\n\r\n    const response = await dispatch(loginUser(body));\r\n\r\n    if (response.LoginSuccess) {\r\n      props.history.push(\"/\");\r\n    } else {\r\n      alert(\"Error Occured\");\r\n    }\r\n\r\n    // NavBar 같은 곳에서 로그인 이후에 reducer에 존재하는 user에 user_id를 할당해주면 될 것 같음. 그리고 나머지 요소들에서 로그인 여부에 따른 처리를 해주면 될 것 같음.\r\n  };\r\n\r\n  return (\r\n    <LoginForm onSubmit={onSubmit}>\r\n      <InputContainer>\r\n        <BsFillPersonFill />\r\n        <Input\r\n          type=\"id\"\r\n          placeholder=\"ID를 입력하세요\"\r\n          onChange={onIdChange}\r\n          required\r\n        ></Input>\r\n      </InputContainer>\r\n      <InputContainer>\r\n        <AiFillLock />\r\n        <Input\r\n          type=\"password\"\r\n          placeholder=\"비밀번호를 입력하세요\"\r\n          onChange={onPasswordChange}\r\n          autoComplete=\"cc-number\"\r\n          required\r\n        ></Input>\r\n      </InputContainer>\r\n      <ButtonContainer>\r\n        <Button type=\"submit\">로그인</Button>\r\n        {/* 로그인 안된 상태에서만 가능하도록 처리해줘야 함. */}\r\n        <a href=\"/join\">\r\n          <Button>회원가입</Button>\r\n        </a>\r\n      </ButtonContainer>\r\n    </LoginForm>\r\n  );\r\n};\r\n\r\nexport default withRouter(Login);\r\n","C:\\Users\\donxr\\Documents\\GitHub\\react-express-js\\client