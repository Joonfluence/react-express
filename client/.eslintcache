   </SavingGoalBlock>\r\n  );\r\n};\r\n\r\nexport default Auth(SavingGoal, true);\r\n","C:\\Users\\donxr\\Documents\\GitHub\\react-express-js\\client\\src\\Components\\pages\\Budget\\Budget.js",["217"],"import React, { useEffect } from \"react\";\r\nimport { useSelector, useDispatch } from \"react-redux\";\r\nimport { Helmet } from \"react-helmet\";\r\nimport Footer from \"../../common/Footer\";\r\nimport styled from \"styled-components\";\r\nimport { monthBudget } from \"../../../redux/actions/budgetActions\";\r\nimport CenterButton from \"../../common/CenterButton\";\r\nimport Header from \"../../common/Header\";\r\nimport ContentBody from \"../../common/ContentBody\";\r\nimport ContentHeader from \"../../common/ContentHeader\";\r\nimport Auth from \"../../../hoc/auth\";\r\nimport CreateBoard from \"../../common/CreateBoard\";\r\nimport DateModal from \"../../utils/DateModal\";\r\n\r\nconst BudgetBlock = styled.div`\r\n  .content {\r\n    margin: 0 auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background-color: ${(props) => props.theme.color.white};\r\n  }\r\n\r\n  .modal__container {\r\n    width: 10%;\r\n  }\r\n`;\r\n\r\nconst Budget = () => {\r\n  const dispatch = useDispatch();\r\n\r\n  const { yearInfo, monthInfo } = useSelector(({ dateReducer }) => ({\r\n    yearInfo: dateReducer.yearInfo,\r\n    monthInfo: dateReducer.monthInfo,\r\n  }));\r\n\r\n  const { budgetSum, monthlyBudget, payingSum } = useSelector(\r\n    ({ budgetReducer }) => ({\r\n      budgetSum: budgetReducer.budgetSum,\r\n      monthlyBudget: budgetReducer.monthlyBudget,\r\n      payingSum: budgetReducer.categorySum,\r\n    })\r\n  );\r\n\r\n  useEffect(() => {\r\n    let body = {\r\n      year: yearInfo,\r\n      month: monthInfo,\r\n    };\r\n    async function fetchPageData() {\r\n      await dispatch(monthBudget(body));\r\n    }\r\n    fetchPageData();\r\n  }, [yearInfo, monthInfo]);\r\n\r\n  const pocketMoney = budgetSum - payingSum;\r\n\r\n  return (\r\n    <BudgetBlock>\r\n      <Helmet>\r\n        <title>이번달 소비관리 | 돼지저금통</title>\r\n      </Helmet>\r\n      <Header />\r\n      <CenterButton />\r\n      <main className=\"content\">\r\n        <CreateBoard isBudget={true} InfoName=\"예산\" />\r\n        <div className=\"modal__container\">\r\n          <DateModal yearInfo={yearInfo} monthInfo={monthInfo} />\r\n        </div>\r\n        <div>\r\n          {payingSum === 0 && budgetSum === 0 ? (\r\n            <></>\r\n          ) : (\r\n            <ContentHeader\r\n              yearInfo={yearInfo}\r\n              monthInfo={monthInfo}\r\n              used={payingSum}\r\n              remained={pocketMoney}\r\n              isBudget={true}\r\n              budgetSum={budgetSum}\r\n            ></ContentHeader>\r\n          )}\r\n          {monthlyBudget !== null ? (\r\n            <ContentBody\r\n              budgetSum={budgetSum}\r\n              monthlyData={monthlyBudget}\r\n              used={payingSum}\r\n              remained={pocketMoney}\r\n              isBudget={true}\r\n              isCategory={false}\r\n            ></ContentBody>\r\n          ) : (\r\n            <></>\r\n          )}\r\n        </div>\r\n      </main>\r\n      <Footer />\r\n    </BudgetBlock>\r\n  );\r\n};\r\n\r\nexport default Auth(Budget, true);\r\n","C:\\Users\\donxr\\Documents\\GitHub\\react-express-js\\client\\src\\Components\\pages\\Introduction\\Introduce.js",[],"C:\\Users\\donxr\\Documents\\GitHub\\react-express-js\\client\\src\\Components\\charts\\CategoryBlock.js",[],"C:\\Users\\donxr\\Documents\\GitHub\\react-express-js\\client\\src\\Components\\charts\\PieTotalRatio.js",[],"C:\\Users\\donxr\\Documents\\GitHub\\react-express-js\\client\\src\\Components\\charts\\MonthDataList.js",["218"],"import React, { useEffect } from \"react\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport styled from \"styled-components\";\r\nimport CategoryBlock from \"./Categor